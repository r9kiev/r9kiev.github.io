>\n        <input type=\"color\" title=\"Overlay Color 2\" name=\"overlayColor2\" value=\"#ff0d00\" condition=\"showEffect &amp;&amp; imagesAmount > '1'\">\n        <input type=\"color\" title=\"Overlay Color 3\" name=\"overlayColor3\" value=\"#7d3ed5\" condition=\"showEffect &amp;&amp; imagesAmount >'2'\">\n        <input type=\"color\" title=\"Overlay Color 4\" name=\"overlayColor4\" value=\"#21ac13\" condition=\"showEffect &amp;&amp; imagesAmount >'3'\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"showEffect\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"container-fluid\">\n        <div class=\"slider-box\" data-autoplay=\"{{autoplay}}\" data-delay=\"{{delay}}\"><amp-carousel class=\"carousel\" height=\"9\" width=\"16\" layout=\"responsive\" type=\"slides\" loop=\"\"><div class=\"carousel-img mobirise-loader mbr-flex mbr-jc-c\">\n                    <div class=\"mbr-overlay\" mbr-if=\"showEffect\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/bg8.jpg\" height=\"9\" width=\"13.5\" layout=\"responsive\" alt=\"Mobirise\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/bg8.jpg\" alt=\"Mobirise\" title=\"\">\n                    </amp-img>\n                    <div class=\"card-box container\" mbr-class=\"{'align-center': alignTitle == 2, 'align-right': alignTitle == 3}\">\n                        <h2 class=\"subtitle mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".subtitle\">Slider Subtitle</h2>\n                        <h1 class=\"title__text mbr-fonts-style mbr-bold mbr-white\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".title__text, .mbr-section-btn\">amazing</h1>\n                        <p class=\